{"ast":null,"code":"import { mapGetters, mapState } from \"vuex\";\nexport default {\n  name: \"BagComponent\",\n  computed: { ...mapState([\"bag\"]),\n    ...mapGetters([\"subTotal\", \"descuento\", \"total\"]),\n\n    totales() {\n      return [{\n        text: \"Subtotal Productos\",\n        total: this.subTotal\n      }, {\n        text: \"Descuentos\",\n        total: this.descuento\n      }, {\n        text: \"Total\",\n        total: this.total\n      }];\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0DA;AACA;EACAA,oBADA;EAEAC,YACA,oBADA;IAEA,iDAFA;;IAGAC;MACA,QACA;QAAAC;QAAAC;MAAA,CADA,EAEA;QAAAD;QAAAC;MAAA,CAFA,EAGA;QAAAD;QAAAC;MAAA,CAHA;IAKA;;EATA;AAFA","names":["name","computed","totales","text","total"],"sourceRoot":"src/components","sources":["BagComponent.vue"],"sourcesContent":["<template>\n  <v-card id=\"BagComponent\" d-flex-align-stretch>\n    <v-card-title v-text=\"`Carro de Compras Vinos Mi Chile`\" />\n    <v-divider />\n    <v-card-text class=\"py-0\">\n      <v-card class=\"my-4\" v-for=\"(product, i) in bag\" :key=\"i\">\n        <div class=\"d-flex flex-no-wrap\">\n          <v-avatar class=\"ma-3\" size=\"125\">\n            <v-img :src=\"product.imagen\"></v-img>\n          </v-avatar>\n          <div>\n            <v-card-title v-text=\"product.nombre\" />\n            <v-card-subtitle\n              class=\"primary--text\"\n              v-text=\"\n                product.valor.toLocaleString('es-cl', {\n                  style: 'currency',\n                  currency: 'clp',\n                })\n              \"\n            />\n          </div>\n        </div>\n      </v-card>\n    </v-card-text>\n    <v-divider />\n    <v-card-text>\n      <v-row >\n        <template v-for=\"(total, i) in totales\">\n          <v-col cols=\"6\" :key=\"`text${i}`\" v-text=\"total.text\" />\n          <v-col\n            class=\"text-h6\"\n            :class=\"{ 'primary--text': total.text == `Total` }\"\n            cols=\"6\"\n            :key=\"`total${i}`\"\n            v-text=\"\n              total.total.toLocaleString('es-cl', {\n                style: 'currency',\n                currency: 'clp',\n              })\n            \"\n          />\n        </template>\n      </v-row>\n      <v-btn\n        v-if=\"$route.name != 'checkout'\"\n        class=\"mt-4\"\n        color=\"primary\"\n        v-text=\"`pagar`\"\n        block\n        @click=\"$router.push({ name: 'checkout' })\"\n        :disabled=\"bag.length < 1\"\n      />\n    </v-card-text>\n  </v-card>\n</template>\n\n<script>\nimport { mapGetters, mapState } from \"vuex\";\nexport default {\n  name: \"BagComponent\",\n  computed: {\n    ...mapState([\"bag\"]),\n    ...mapGetters([\"subTotal\", \"descuento\", \"total\"]),\n    totales() {\n      return [\n        { text: \"Subtotal Productos\", total: this.subTotal },\n        { text: \"Descuentos\", total: this.descuento },\n        { text: \"Total\", total: this.total },\n      ];\n    },\n  },\n};\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}